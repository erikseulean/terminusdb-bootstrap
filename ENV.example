# shellcheck shell=sh
# shellcheck disable=SC2034

#
# ENVIRONMENT
#
# To persist environment settings copy this file to ENV
#

# Docker command
#TERMINUSDB_DOCKER=sudo docker

# Container 
#TERMINUSDB_CONTAINER=terminus-server
#TERMINUSDB_REPOSITORY=terminusdb/terminus-server
#TERMINUSDB_TAG=latest
#TERMINUSDB_NETWORK=bridge
#TERMINUSDB_LABEL_FILE=labels

# Container Port to Publish
#TERMINUSDB_PORT=6363

# Local Directory to Mount inside Container
#TERMINUSDB_LOCAL=/home/username/localfiles

# Server
#TERMINUSDB_SCHEME=http
#TERMINUSDB_SERVER=localhost
#TERMINUSDB_SERVER_PORT=6363
#TERMINUSDB_PASS=root
#TERMINUSDB_AUTOATTACH=true
#TERMINUSDB_AUTOLOGIN=false
#TERMINUSDB_CONSOLE=http://localhost/console

# Server Public Url
#TERMINUSDB_PUBLIC_URL=https://example.com
TERMINUSDB_CONSOLE_BASE_URL="https://dl.bintray.com/terminusdb/terminusdb/0.0.1"

# vim:ft=sh
